        "rate-limit",
        "cors"
    ],
    "author": "DRVGO",
    "license": "ISC",
    "dependencies": {
        "@paralleldrive/cuid2": "^2.2.2",
        "body-parser": "^1.20.2",
        "chalk": "^4.1.2",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "multer": "1.4.5-lts.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
        "@types/body-parser": "^1.19.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.4",
        "@types/react": "^18.2.47",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "prettier": "^3.1.1",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
