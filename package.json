{
    "name": "express-ts-template",
    "version": "1.0.0",
    "description": "A template for express with typescript",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon",
        "build": "rimraf build && tsc",
        "lint": "eslint --ext .ts src",
        "start": "cross-env NODE_ENV=production node --env-file=.env build/index.js",
        "test": "jest"
    },
    "type": "commonjs",
    "keywords": [
        "express",
        "typescript",
        "template",
        "api",
        "file-upload",
